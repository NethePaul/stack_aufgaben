        <questionvariables>
            <text>u1: 1+rand(3)
u2: -1-rand(4)
u3: 0
uv: matrix([u1],[u2],[u3]);
v1: 1
v2: 1+rand(3)
v3: 1
vv: matrix([v1],[v2],[v3]);
w1: -1-rand(3)
w2: 0
w3: 1+rand(3)
wv: matrix([w1],[w2],[w3]);

M: matrix([u1,v1,w1], [u2,v2,w2], [u3,v3,w3]);

r: rank(M);


x1: rand_with_step(-9,9,1);
x2: rand_with_step(-9,9,1);
x3: rand_with_step(-9,9,1);
xv: matrix([x1],[x2],[x3]);
Minv: invert(M);

Bx1: (Minv.[x1,x2,x3])[1][1];
Bx2: (Minv.[x1,x2,x3])[2][1];
Bx3: (Minv.[x1,x2,x3])[3][1];</text>
